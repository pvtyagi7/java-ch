  </servlet>
  <servlet-mapping>
  	<servlet-name>genericServlet1</servlet-name>
  	<url-pattern>/generic1</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>helloUser1</servlet-name>
